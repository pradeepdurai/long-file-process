<div class="container mt-4 p-5 alert alert-primary">
    <h5>Edit Product</h5>
   <form id="csvForm" action="/list/update" method="post" enctype="multipart/form-data" class="mt-5">
   
    <div class="form-group">
    <label for="Import Csv" class="text-default">Name</label>
    <input type="text" name="name" class="form-control" value="{{#each data}}{{#if this.name}}{{this.name}}{{/if}}{{/each}}" placeholder="Enter Name">
  </div>
   <div class="form-group">
    <label for="Import Csv" class="text-default">SKU</label>
    <input type="text" name="sku" class="form-control"  value="{{#each data}}{{#if this.sku}}{{this.sku}}{{/if}}{{/each}}" placeholder="Enter SKU">
  </div>
   <div class="form-group">
    <label for="Import Csv" class="text-default">Description</label>
    <input type="text" name="desc" class="form-control"  value="{{#each data}}{{#if this.description}}{{this.description}}{{/if}}{{/each}}" placeholder="Enter Description">
  </div>
  <div class="row">
      <div class="col-1"> <button type="submit" class="btn btn-primary">Submit</button></div>
      <div class="col-2"><div class="loader " style="display:none"></div></div>
  </div>
   </form>
</div>